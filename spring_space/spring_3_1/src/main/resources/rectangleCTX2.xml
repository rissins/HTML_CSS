<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
       
       <bean id = "rectangle" class ="com.javalec.spring_3_Ex7.Rectangle"></bean>
       <bean id = "myRectangle" class="com.javalec.spring_3_Ex7.MyRectangle">
       		<property name="rectangle" ref ="rectangle"></property>
       		<property name="x" value="150"></property>
       		<property name="y" value="150"></property>
       </bean>
       
</beans>       